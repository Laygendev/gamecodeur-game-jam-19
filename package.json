{
  "name": "tankio",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/server/index.js",
    "build": "babel -x \".ts\" server/ -d dist/server && babel -x \".ts\" shared/ -d dist/shared && babel -x \".ts\" src/ -d dist/src",
    "build-webpack": "webpack -w",
    "serve": "node dist/index.ts",
    "typecheck": "tsc",
    "standard": "node \"node_modules/standard/bin/cmd.js\" server/*.ts src/*.ts shared/*.ts"
  },
  "author": "BwooGames <contact@bwoogames.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "vectors": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.4.0",
    "@types/express": "^4.16.1",
    "@types/hashmap": "^2.0.29",
    "@types/node": "^11.13.0",
    "@types/socket.io": "2.1.2",
    "@types/socket.io-client": "1.4.32",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-loader": "8.0.5",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "^1.18.10",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard": "12.0.1",
    "ts-loader": "5.3.3",
    "typescript": "^3.4.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "typescript"
    ],
    "ignore": [
      "**/lib/",
      "**/out/",
      "**/dist/"
    ]
  }
}
